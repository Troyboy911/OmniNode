{
  "name": "omni-node-backend",
  "version": "1.0.0",
  "description": "Backend API for Omni Node AI Orchestration Platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:ai": "jest --testPathPattern=ai",
    "test:ai:openai": "jest --testPathPattern=ai --testNamePattern='OpenAI'",
    "test:ai:claude": "jest --testPathPattern=ai --testNamePattern='Claude'",
    "test:ai:ollama": "jest --testPathPattern=ai --testNamePattern='Ollama'",
    "test:ai:health-report": "node scripts/generate-ai-health-report.js",
    "test:performance": "jest --testPathPattern=performance",
    "test:load": "jest --testPathPattern=load",
    "test:coverage": "jest --coverage",
    "test:smoke": "jest --testPathPattern=smoke",
    "test:e2e": "jest --testPathPattern=e2e",
    "benchmark:ai": "node scripts/benchmark-ai.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "snyk test",
    "docker:build": "docker build -t omninode-backend .",
    "docker:run": "docker run -p 3001:3001 omninode-backend",
    "health:check": "node scripts/health-check.js",
    "metrics:generate": "node scripts/generate-metrics.js",
    "update:model-registry": "node scripts/update-model-registry.js"
  },
  "keywords": [
    "ai",
    "orchestration",
    "agents",
    "blockchain"
  ],
  "author": "Omni Node Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
