{
  "name": "omninode-frontend",
  "production_branch": "main",
  "build": {
    "command": "npm run build",
    "destination_dir": ".next",
    "root_dir": "/",
    "environment_variables": {
      "NODE_VERSION": "20",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "deployment": {
    "compatibility_date": "2024-01-01",
    "compatibility_flags": ["nodejs_compat"],
    "environment_variables": {
      "NEXT_PUBLIC_API_URL": "https://api.omninode.app",
      "NEXT_PUBLIC_WS_URL": "wss://api.omninode.app"
    }
  },
  "preview": {
    "environment_variables": {
      "NEXT_PUBLIC_API_URL": "https://api-staging.omninode.app",
      "NEXT_PUBLIC_WS_URL": "wss://api-staging.omninode.app"
    }
  },
  "domains": [
    {
      "name": "omninode.app",
      "type": "apex"
    },
    {
      "name": "www.omninode.app",
      "type": "subdomain"
    }
  ]
}